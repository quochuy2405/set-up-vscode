	"debug.inlineValues": "on",
	"editor.codeActionsOnSave": {},
	"autoprefixer.options": {
		"browsers": ["last 4 versions", "ie >= 9", "> 5%"]
	},
	"intelephense.format.braces": "k&r",
	"intelephense.environment.shortOpenTag": false,
