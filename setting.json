{
    "editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"tabnine.experimentalAutoImports": true,
	"terminal.integrated.fontFamily": "Cascadia Code,Consolas, 'Courier New', monospace",
	"editor.fontSize": 17,
	"workbench.iconTheme": "material-icon-theme",
	"html.format.endWithNewline": true,
	"liveServer.settings.ignoreFiles": ["*/.json", "*.json"],
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"editor.wordWrap": "on",
	"prettier.jsxSingleQuote": true,
	"prettier.jsxBracketSameLine": true,
	"prettier.singleQuote": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.editorAssociations": {
		"*.dll": "default"
	},
	"liveServer.settings.donotVerifyTags": true,
	"prettier.useTabs": true,
	"explorer.confirmDelete": false,
	"prettier.proseWrap": "always",
	"powermode.enabled": true,
	"powermode.enableStatusBarComboTimer": false,
	"workbench.colorTheme": "Dracula darker contrast",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"files.autoSaveDelay": 100,
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"editor.formatOnSave": true,
	"explorer.confirmDragAndDrop": false,
	"powermode.combo.location": "statusbar",
	"powermode.combo.counterEnabled": "hide",
	"powermode.combo.timerEnabled": "hide",
	"powermode.shake.enabled": false,
	"powermode.explosions.size": 3.5,
	"powermode.explosions.offset": 0.2,
	"sonarlint.rules": {
		"Web:BoldAndItalicTagsCheck": {
			"level": "off"
		},
		"typescript:S2814": {
			"level": "off"
		},
		"javascript:S3317": {
			"level": "off"
		},
		"python:S1542": {
			"level": "off"
		}
    },
    "editor.fontFamily": "\"Cascadia Code\",Consolas, 'Courier New', monospace",
}
