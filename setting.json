{
	// Open the merge editor for files that are currently under conflict.
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"mergeEditor.showCheckboxes": true,
	"mergeEditor.showCodeLenses": false,
	"tabnine.experimentalAutoImports": true,
	"terminal.integrated.fontFamily": "Cascadia Code,Consolas, 'Courier New', monospace",
	"editor.fontSize": 14,
	"html.format.endWithNewline": true,
	"liveServer.settings.ignoreFiles": ["*/.json", "*.json"],
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"editor.wordWrap": "on",
	"prettier.jsxSingleQuote": false,
	"prettier.jsxBracketSameLine": false,
	"prettier.singleQuote": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.editorAssociations": {
		"*.dll": "default"
	},
	"liveServer.settings.donotVerifyTags": true,
	"prettier.useTabs": true,
	"explorer.confirmDelete": false,
	"prettier.proseWrap": "always",
	"powermode.enabled": true,
	"powermode.enableStatusBarComboTimer": false,
	"workbench.colorTheme": "Evondev Dracula Darker Contrast",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"files.autoSaveDelay": 100,
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"editor.formatOnSave": true,
	"explorer.confirmDragAndDrop": false,
	"powermode.combo.location": "statusbar",
	"powermode.combo.counterEnabled": "hide",
	"powermode.combo.timerEnabled": "hide",
	"powermode.shake.enabled": false,
	"powermode.explosions.size": 3.5,
